{
  "name": "shri-boilerplate",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001/",
  "dependencies": {
    "concurrently": "^3.5.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "mongod": "^2.0.0",
    "mongodb": "^3.0.3",
    "nodemon": "^1.15.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "socket.io-cookie-parser": "^1.0.0",
    "uuid": "^3.2.1"
  },
  "scripts": {
    "start": "concurrently \"yarn client\" \"yarn server\"",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "client": "react-scripts start",
    "server": "nodemon -w server.js -w src/server server.js",
    "storybook": "start-storybook -p 9999 -c src/storybook",
    "postinstall": "git config core.hooksPath hooks",
    "eslint": "./node_modules/.bin/eslint"
  },
  "devDependencies": {
    "@storybook/react": "^3.3.15",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0"
  }
}
