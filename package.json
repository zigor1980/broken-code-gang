{
  "name": "shri-boilerplate",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001/",
  "engines": {
    "node": "9.11.1"
  },
  "dependencies": {
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "mongod": "^2.0.0",
    "mongodb": "^3.0.3",
    "nodemon": "^1.15.1",
    "react": "^16.3.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.1",
    "react-test-renderer": "^16.3.1",
    "redux": "^3.7.2",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "socket.io-cookie-parser": "^1.0.0",
    "uuid": "^3.2.1"
  },
  "scripts": {
    "start": "concurrently \"npm run client\" \"npm run server\"",
    "build": "react-scripts build",
    "jest": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "client": "react-scripts start",
    "server": "nodemon -w server.js -w src/server server.js",
    "storybook": "start-storybook -p 9999 -c src/storybook",
    "githooks": "git config core.hooksPath hooks",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "test": "cross-env NODE_ENV=test  mocha --compilers js:babel-core/register src/**/*.spec.js",
    "test:watch": "npm test -- -w",
    "gemini:gui": "gemini-gui -c ./.gemini.js",
    "gemini": "gemini test -c ./.gemini.js"
  },
  "devDependencies": {
    "@storybook/react": "^3.3.15",
    "babel-core": "^6.26.0",
    "babel-preset-react-app": "^3.1.1",
    "chai-enzyme": "^0.8.0",
    "cross-env": "^5.1.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.7.0",
    "gemini": "^5.6.2",
    "gemini-gui": "^6.0.0",
    "mocha": "^5.1.0",
    "selenium-standalone": "^6.14.0"
  }
}
